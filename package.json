    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sanity": "^3.48.1",
    "styled-components": "^6.1.11",
    "typescript": "^5.4"
  },
  "peerDependencies": {
    "react": "^18",
    "sanity": "^3"
  },
  "engines": {
    "node": ">=18"
